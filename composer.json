    "type": "flarum-extension",
    "keywords": ["flarum", "redis", "session"],
    "license": "proprietary",
    "autoload": {
        "psr-4": {
            "PixelFederation\\RedisSession\\": "src/"
        }
    },
    "require": {
        "flarum/core": "^0.1.0-beta.3",
        "predis/predis": "^1.1"
